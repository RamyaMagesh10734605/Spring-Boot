
kind: Deployment
apiVersion: apps/v1
metadata:
  name: spring-boot
spec:
  template:
    spec:
      containers:
      - name: spring-boot
        image: gcr.io/$PROJECT_ID/github.com/ramyamagesh10734605/spring-boot:$COMMIT_SHA
        resources:
          requests:
            cpu: 25m
            memory: 64Mi
          limits:
            cpu: 100m
            memory: 256Mi
        env:
        - name: ENVIRONMENT
          value: prod